<div id="create_event">
    <h1>Create New Event</h1>

  <form id="create_event_form" action="/events/create" method="post">

    <p><input class="input_form" type="text" name="event[title]"> Title</p>
    <p><input class="input_form" type="text" name="event[organizer_name]"> Organizer Name</p>
    <p><input class="input_form" type="text" name="event[organizer_email]"> Organizer Email</p>
    <p><input class="input_form" type="date" name="event[date]"> Date</p>

    <input class="submit_button" id="submit_new_note" type="submit">
  </form>
</div>
